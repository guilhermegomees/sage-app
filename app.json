{
  "expo": {
    "name": "sage",
    "slug": "sage",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./src/assets/images/sage.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./src/assets/images/sage.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/load.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/sage.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
